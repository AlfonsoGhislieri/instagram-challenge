deletePost = () => {
  let thingToDelete = document.getElementById("post-<%=@post.id%>");
  thingToDelete.className = "is-success notification is-light has-text-bold alert alert-success"
  thingToDelete.innerHTML = "";
  thingToDelete.innerText = "Post deleted";
  thingToDelete.setAttribute("id", "flashmessage")
}

deletePost();